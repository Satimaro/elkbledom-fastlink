{
  "domain": "elkbledom",
  "name": "ELK-BLEDOM FastLink",
  "codeowners": ["@Satimaro"],
  "config_flow": true,
  "dependencies": ["bluetooth"],
  "documentation": "https://github.com/Satimaro/elkbledom-fastlink",
  "iot_class": "local_polling",
  "issue_tracker": "https://github.com/Satimaro/elkbledom-fastlink/issues",
  "requirements": [
    "bleak-retry-connector>=3.5.0",
    "bleak>=0.22.2"
  ],
  "version": "1.3.0",
  "after_dependencies": ["hassio", "homeassistant"],
  "integration_type": "device",
  "loggers": ["custom_components.elkbledom"],
  "bluetooth": [
    { "local_name": "ELK-BLEDDM*" },
    { "local_name": "ELK-BULB2*" },
    { "local_name": "ELK-BULB*" },
    { "local_name": "ELK-BLE*" },
    { "local_name": "MELK*" },
    { "local_name": "LEDBLE*" },
    { "local_name": "ELK-LAMPL*" }
  ]
}
